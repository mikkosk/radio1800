{
  "compilerOptions": {
      "outDir": "./build/",
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "jsx": "react-jsx",
  },
  "exclude": ["src"]
}